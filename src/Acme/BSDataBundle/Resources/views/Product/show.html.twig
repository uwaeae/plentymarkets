
{% extends '::base.html.twig' %}
{% block title %}INTERN - Produkt - {{ entity.name }}{% endblock %}


{% block body %}
<h1>{{ entity.name }}</h1>
</div>
</header>
<nav class="ym-hlist">

    <ul>


        <li><a  href="{{ path('BSData_mainmenu') }}">
            zurück
        </a>
        </li>
    </ul>
</nav>
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Article_id</th>
            <td>{{ entity.articleid }}</td>
        </tr>
        <tr>
            <th>Article_no</th>
            <td>{{ entity.articleno }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Price</th>
            <td>{{ entity.price }}</td>
        </tr>

        <tr>
            <th>Label_text</th>
            <td>{{ entity.labeltext }}</td>
        </tr>

    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('BSData_product') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('BSData_product_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('BSData_product_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="button" type="submit">Löschen</button>
        </form>
    </li>
</ul>
        {% endblock %}